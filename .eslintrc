{
  "env": {
      "browser": false,
      "commonjs": true,
      "es6": true
  },
  "extends": [
      "plugin:react/recommended"
    ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    },
    "requireConfigFile": false
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "react/jsx-indent": [
        "error",
        2
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "double"
      ],
      "semi": [
          "warn",
          "always"
      ]
  },
  "parser": "@babel/eslint-parser"
}
